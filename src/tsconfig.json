{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "mapRoot": "/",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "outDir": "../dist/",
    "rootDir": ".",
    "sourceMap": true,
    "target": "es5",
    "inlineSources": true
  },
"exclude": [
        "node_modules",
        "typings/browser.d.ts",
        "typings/browser/**"
    ],
  "files": [
    "main.ts",
    "app.component.ts",
    "ng2-notifications.ts"
  ]
}
